		this.nom = nom;
		this.regard = regard;
	}

	@Override
	public String toString() {
		return "Je suis " + nom;
	}

	@Override
	public int veriforientation(Orientation regard) {
		// TODO Auto-generated method stub
		return 0;
	}

}
