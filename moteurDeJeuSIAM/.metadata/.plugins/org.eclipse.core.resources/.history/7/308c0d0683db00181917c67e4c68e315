package moteur;

public class UnePartie {

	public static void main(String[] args) {
		Plateau unPlateau = new Plateau(5);
		Pion unPion = new Pion("elephant");
		Pion unPion1 = new Pion("rhino");
		Rocher unRocher = new Rocher("cahioux");
		unPlateau.ajouterJeton(unPion, 2, 3);
		unPlateau.ajouterJeton(unRocher, 3, 4);
		unPlateau.afficherPlateau();
		unPlateau.deplacementNord(unPion);
		unPlateau.afficherPlateau();
		
	}

}
