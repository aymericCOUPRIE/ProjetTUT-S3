package moteur;

public class UnePartie {

	public static void main(String[] args) {
		Plateau unPlateau = new Plateau(5);
		Pion unPion = new Pion();
		Pion unPion1 = new Pion();
		Rocher unRocher = new Rocher();
		unPlateau.ajouterJeton(unPion, 2, 3);
		unPlateau.ajouterJeton(unRocher, 3, 4);
		unPlateau.afficherPlateau();
		System.out.println(unPlateau.teste(unPion1));
		System.out.println(unPlateau.teste(unPion));

	}

}
